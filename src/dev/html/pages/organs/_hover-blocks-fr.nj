<section class="organs-sec">
  <div class="organs-wrap">
    <div class="organs">
      <ul class="organs-list">
        {% for item in list %}
        <li>
          <a href="{{ item.href }}" class="organs-list__item">
            <i class="organs-list__item-icon">
              {{ svg.icon(item.icon) }}
            </i>
            <p class="organs-list__item-title">
              {{ item.title | safe}}
            </p>
          </a>

          <div class="organs-list__hover {% if item.extraClass %} organs-list__hover--top {% endif %}">
            <div class="organs-list__hover-inner">
              <p class="organs-list__hover-title">
                {{ item.hoverTitle | safe }}
              </p>
              <div class="organs-list__hover-text">
                {{ item.hoverText | safe }}
              </div>
            </div>
          </div>
        </li>
        {% endfor %}
      </ul>
    </div>
  </div>
</section>